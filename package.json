{
  "name": "@fdm-monster/client-next",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fdm-monster/fdm-monster-client-next.git"
  },
  "author": "David Zwart",
  "license": "AGPL-3.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "description": "FDM Monster Client Next is the Vue3 frontend for FDM Monster.",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "test:unit": "vitest --environment jsdom",
    "test:unit:update": "vitest --environment jsdom -u",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "fetch:openapi": "curl -s http://127.0.0.1:4000/api-docs/swagger.json -o openapi.json",
    "openapi-ts": "openapi-ts",
    "generate:api": "yarn fetch:openapi && yarn openapi-ts"
  },
  "dependencies": {
    "core-js": "3.47.0",
    "material-design-icons-iconfont": "6.7.0",
    "roboto-fontface": "*",
    "vue": "3.5.26",
    "vuetify": "3.11.6"
  },
  "devDependencies": {
    "@babel/types": "7.28.5",
    "@hey-api/client-axios": "0.9.1",
    "@hey-api/openapi-ts": "0.90.0",
    "@mdi/font": "7.4.47",
    "@sentry/tracing": "7.120.4",
    "@sentry/vite-plugin": "4.6.1",
    "@sentry/vue": "10.32.1",
    "@tanstack/vue-query": "5.92.5",
    "@testing-library/vue": "8.1.0",
    "@types/js-yaml": "4.0.9",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.4",
    "@types/semver": "7.7.1",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.51.0",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/ui": "4.0.16",
    "@vitest/web-worker": "4.0.16",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "14.1.0",
    "@vueuse/integrations": "14.1.0",
    "axios": "1.13.2",
    "axios-mock-adapter": "2.1.0",
    "c8": "10.1.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-vue": "10.6.2",
    "eslint-plugin-vuetify": "2.5.3",
    "jsdom": "27.4.0",
    "jwt-decode": "4.0.0",
    "pinia": "3.0.4",
    "prettier": "3.7.4",
    "sass-embedded": "1.97.1",
    "selective-object-reuse": "1.0.6",
    "semver": "7.7.3",
    "socket.io-client": "4.8.3",
    "typescript": "5.9.3",
    "unplugin-auto-import": "20.3.0",
    "unplugin-fonts": "1.4.0",
    "unplugin-vue-components": "30.0.0",
    "vite": "7.3.0",
    "vite-plugin-vuetify": "2.1.2",
    "vitest": "4.0.16",
